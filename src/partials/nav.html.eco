<nav>
    <ul class="list">
        <% for item in @nav: %>
            <li class="list-item">
                <a href="<%= item.href %>" title="<%- item.title || item.text %>"
                    <% if item.external: %>target="_blank" <%= end %>
                    class="list-item-link"><%- item.text %></a>
            </li>
        <% end %>
    </ul>
</nav>
